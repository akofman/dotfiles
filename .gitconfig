[color]
   branch = auto
   diff = auto
   status = auto
    ui = 1
[color "branch"]
   current = yellow reverse
   local = yellow
   remote = green
[color "diff"]
   meta = yellow bold
   frag = magenta bold
   old = red bold
   new = green bold
[color "status"]
   added = yellow
   changed = green
   untracked = cyan
[user]
   name = Alexis Kofman
   email = alexis.kofman@gmail.com
[filter "media"]
   required = true
   clean = git media clean %f
   smudge = git media smudge %f
[url "http://"]
   insteadOf = git://
[alias]
   logcat = !git --no-pager log -n 5 --pretty=oneline
[merge]
   tool = opendiff
[credential]
   helper = osxkeychain
